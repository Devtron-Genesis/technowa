name = Tech Nowa
description = Tech Nowa Responsive Drupal 7 Theme.
core = 7.x
engine = phptemplate

base theme = hasta

regions[top]                = Top
regions[content_top]        = Content Top
regions[content]            = Content
regions[sidebar]            = Sidebar
regions[content_full]       = Content Full Width
regions[bottom]             = Content Bottom
regions[footer]             = Footer

features[] = name
features[] = logo
features[] = slogan
features[] = node_user_picture
features[] = comment_user_picture
features[] = comment_user_verification
features[] = favicon
features[] = main_menu

settings[toggle_name] = 1
settings[toggle_logo] = 1
settings[toggle_slogan] = 1
settings[toggle_node_user_picture] = 1
settings[toggle_comment_user_picture] = 1
settings[toggle_comment_user_verification] = 1
settings[toggle_favicon] = 1
settings[toggle_main_menu] = 1
settings[skin] = default

settings[loader_image] = 1

;;;;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets & Scripts
;;;;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/style.css
stylesheets[all][] = css/rwd.css

scripts[] = js/jquery.matchHeight-min.js
scripts[] = js/jquery.event.move.js
scripts[] = js/protip.min.js
scripts[] = js/jquery.scrollTo.min.js
scripts[] = js/jquery.flipster.js
scripts[] = js/jquery.viewportchecker.js
scripts[] = js/attrchange.js
scripts[] = js/jquery.mobile-events.js
scripts[] = js/script.js

version = "7.x-1.1"
core = "7.x"
project = "Hasta"
